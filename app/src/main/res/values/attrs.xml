<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ArialRound">
        <attr name="text_type" format="enum">
            <enum name="thin" value="0" />
            <enum name="bold" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyViewStyle">
        <attr name="show_type" format="boolean"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
    </declare-styleable>
</resources>